test: {{ mapper.default_test_run }}
duration: {{ mapper.default_test_timeout }}
tty: true
order: 200
{% for test in testmapper %}
/{{ test.testname }}:
    {%- if test.run %}
    test: /{{ test.run }}
    {%- endif %}
    {%- if test.dir != '.' %}
    path: /{{ test.dir }}
    {%- endif %}
    {%- if test.tags %}
    tag: {{ test.tags }}
    {%- endif %}
    order+: {{ test.order }}
    {%- if test.timeout %}
    duration: {{ test.timeout }}
    {%- endif %}
{% endfor %}
